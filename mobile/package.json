{
  "name": "OptionsScanner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "android": "react-native run-android",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod-install": "cd ios && pod install && cd ..",
    "clean": "cd ios && xcodebuild clean && cd .. && rm -rf node_modules && npm install && npm run pod-install",
    "build:ios": "cd ios && xcodebuild -workspace OptionsScanner.xcworkspace -scheme OptionsScanner -configuration Release -sdk iphoneos -archivePath build/OptionsScanner.xcarchive archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.1",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-svg": "^14.1.0",
    "react-native-svg-charts": "^5.4.0",
    "axios": "^1.6.5",
    "react-native-config": "^1.5.1",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-vector-icons": "^10.0.3",
    "@react-native-picker/picker": "^2.6.1",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-linear-gradient": "^2.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@react-native/babel-preset": "^0.73.19",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.73.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
